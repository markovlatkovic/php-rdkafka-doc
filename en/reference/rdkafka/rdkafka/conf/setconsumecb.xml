<refentry xml:id="rdkafka-conf.setconsumecb" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>RdKafka\Conf::setConsumeCb</refname>
  <refpurpose>Set consume callback</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>void</type><methodname>RdKafka\Conf::setConsumeCb</methodname>
   <methodparam><type>callable</type><parameter>callback</parameter></methodparam>
  </methodsynopsis>
  <para>
   Set consume callback to use with poll
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>callback</parameter> (<type>callable</type>)</term>
    <listitem>
     <para>
      A <type>callable</type> with the following signature:
      <programlisting role="php">
<![CDATA[
<?php
/**
 * @param Message $msg
 */
function ($msg);
]]>
      </programlisting>
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

</refentry>

