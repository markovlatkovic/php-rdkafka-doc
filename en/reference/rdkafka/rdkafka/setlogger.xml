<refentry xml:id="rdkafka.setlogger" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>RdKafka::setLogger</refname>
  <refpurpose>Choose logger</refpurpose>
  <warning>
   <simpara>This function has been <emphasis>DEPRECATED</emphasis>. Relying on this feature is highly discouraged.</simpara>
  </warning>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>void</type><methodname>RdKafka::setLogger</methodname>
   <methodparam><type>integer</type><parameter>logger_id</parameter></methodparam>
  </methodsynopsis>
  <para>
   Logs according to logger id
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>logger_id</parameter> (<type>integer</type>)</term>
    <listitem>
     <para>
      Can be either <constant>RD_KAFKA_PARTITION_UA</constant> (print to stderr), <constant>RD_KAFKA_LOG_SYSLOG</constant> (print to syslog), <constant>RD_KAFKA_LOG_SYSLOG_PRINT</constant> (both).
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns no values.
  </para>
 </refsect1>

</refentry>

